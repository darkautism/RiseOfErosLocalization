<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>RiseOfErosLocalization</AssemblyName>
    <Description>RiseOfEros Localization changer</Description>
    <Version>1.0.0</Version>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BepInEx.IL2CPP" Version="6.0.0-*" IncludeAssets="compile" />
    <PackageReference Include="BepInEx.PluginInfoProps" Version="1.*" />
    <PackageReference Include="LastOrigin.GameLibs" Version="2.3.8-r.0" />
    <PackageReference Include="NuGet.CommandLine" Version="6.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="RiseOfEros.GameLibs" Version="1.0.0" />
  </ItemGroup>
  <ItemGroup>
  <Reference Include="MyAssembly">
    <HintPath>lib\Assembly-CSharp.dll</HintPath>
  </Reference>
</ItemGroup>
</Project>
